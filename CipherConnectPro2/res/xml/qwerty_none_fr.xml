        <Key android:codes="112" android:keyLabel="p" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="113" android:keyLabel="q" android:keyEdgeFlags="left" android:horizontalGap="5%p"/>
        <Key android:codes="115" android:keyLabel="s" android:popupCharacters="ß§śšş" android:popupKeyboard="@xml/qwerty_popup_template"/>
        <Key android:codes="100" android:keyLabel="d" android:popupCharacters="ďđ" android:popupKeyboard="@xml/qwerty_popup_template"/>
