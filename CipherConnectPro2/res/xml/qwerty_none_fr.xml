        <Key android:codes="33" android:keyLabel="!"/>
        <Key android:codes="63" android:keyLabel="@string/sign1" android:popupCharacters="¿" android:popupKeyboard="@xml/qwerty_popup_template"/>
        
    	<Key android:codes="-5" 
             android:keyIcon="@drawable/sym_keyboard_delete" 
             android:iconPreview="@drawable/prv_keyboard_delete"
             android:keyWidth="9%p" 
             android:isRepeatable="true"
             android:keyEdgeFlags="right"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3" 
             android:keyIcon="@drawable/sym_keyboard_done" 
             android:iconPreview="@drawable/prv_keyboard_done"
             android:keyWidth="15%p" 
             android:keyEdgeFlags="left"/>
        
        <Key android:codes="-2" android:keyLabel="123" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        
        <Key android:codes="32" 
             android:keyIcon="@drawable/sym_keyboard_space" 
             android:iconPreview="@drawable/prv_keyboard_space"
             android:keyWidth="35%p" 
             android:isRepeatable="true"/>
        
        <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="15%p"/>
        
        <Key android:codes="10" 
             android:keyIcon="@drawable/sym_keyboard_return" 
             android:iconPreview="@drawable/prv_keyboard_return"
             android:keyWidth="20%p" 
             android:keyEdgeFlags="right"/>
        
    </Row>
