    <Row>
        <Key android:codes="-1" 
             android:keyIcon="@drawable/sym_keyboard_shift" 
             android:iconPreview="@drawable/prv_keyboard_shift"
             android:keyWidth="9%p" 
             android:isModifier="true"
             android:isSticky="true" 
             android:keyEdgeFlags="left"/>
        <Key android:codes="119" android:keyLabel="w"/>
        <Key android:codes="120" android:keyLabel="x"/>
        <Key android:codes="99" android:keyLabel="c" android:popupCharacters="çćč" android:popupKeyboard="@xml/qwerty_popup_template"/>
        <Key android:codes="118" android:keyLabel="v"/>
        <Key android:codes="98" android:keyLabel="b"/>
        <Key android:codes="110" android:keyLabel="n" android:popupCharacters="ňņńñ" android:popupKeyboard="@xml/qwerty_popup_template"/>
